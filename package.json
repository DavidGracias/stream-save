{
  "name": "stream-save",
  "version": "1.0.0",
  "description": "Stream Save - A Stremio addon for saving custom stream links",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "NODE_ENV=development node --experimental-modules server/index.js",
    "build:frontend": "cd frontend && npm run build",
    "start": "NODE_ENV=production node server/index.js",
    "clean": "rm -rf frontend/dist",
    "lint": "cd frontend && npm run lint",
    "type-check": "cd frontend && npm run typecheck",
    "test": "cd frontend && npm test",
    "install-frontend": "cd frontend && npm install"
  },
  "keywords": [
    "stremio",
    "addon",
    "streaming",
    "movies",
    "series",
    "react",
    "flask",
    "mongodb"
  ],
  "author": "Stream Save Team",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "fs": "^0.0.1-security",
    "mongodb": "^6.18.0",
    "node-fetch": "^3.3.2",
    "path": "^0.12.7",
    "url": "^0.11.4",
    "vite": "^7.1.1"
  }
}
